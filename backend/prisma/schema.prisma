datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Lieferschein {
  id           String   @id @default(uuid())
  lfdNr        Int
  eingang      DateTime
  ausgang      DateTime
  kfzKennzeichen String
  container    String?
  bin          String?
  ladung       String?
  fahrerName   String
  bemerkung    String?
  createdAt    DateTime @default(now())
}

